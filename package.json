    "fp"
  ],
  "dependencies": {
    "bignumber.js": "^9.1.1",
    "date-fns": "^2.25.0",
    "esm-seedrandom": "^3.0.5",
    "lru-memoize": "^1.1.0",
    "raw-loader": "^4.0.2",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@calculang/calculang-js": "file:../calculang/packages/calculang-js",
    "eslint": "^8.23.1",
    "lru-memoize": "^1.1.0",
    "source-map": "^0.6.1",
    "underscore": "^1.13.2",
    "webpack-cli": "^5.0.1"
  }
}
